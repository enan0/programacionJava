<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <servlet-name>My Servlet</servlet-name>
    <servlet-class>ar.com.maxo.servlet.MyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>My Servlet</servlet-name>
    <url-pattern>/myservlet</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Hello World Servlet</servlet-name>
    <servlet-class>ar.com.maxo.servlet.HelloWorldServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Hello World Servlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>successButton</servlet-name>
    <servlet-class>ar.com.maxo.servlet.ButtonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>successButton</servlet-name>
    <url-pattern>/success</url-pattern>
  </servlet-mapping>

</web-app>
